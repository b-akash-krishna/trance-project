        "    hadm_ids = cohort_with_notes['HADM_ID'].astype(str).values.astype('S')\n",
        "    f.create_dataset('hadm_ids', data=hadm_ids)\n",
        "\n",
        "emb_df = pd.DataFrame(embeddings, columns=[f'emb_{i}' for i in range(embeddings.shape[1])])\n",
